{
    "manifest_version": 2,
    "name": "Become Exotic",
    "description": "Turns all HTML image tags into photos of our beloved Tiger King.",
    "version": "0.1",
    "browser_action": {
        "default_icon": "img/tiger-king.png",
        "popup": "shopcmp.htm"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["libs/jquery-3.5.0.min.js", "src/joe.js"]
        }
    ]
}